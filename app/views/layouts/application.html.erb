<!DOCTYPE html>
<html>
  <head>
    <title>Untold</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

 
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= favicon_link_tag asset_path('squiggle.png') %>
  </head>
    
  <% if user_signed_in? %>
    <body>
      <nav class="my-nav-bar" style="text-align: right">
        <div class="nav-title"><h2>untold</h2></div>
        <%= link_to "Home", root_path %>
        <%= link_to "Edit Account", edit_user_registration_path %> 
        <%= link_to('Logout', destroy_user_session_path, method: :delete) %>       
      </nav>  
      <%= flash[:alert] %>
      <%= flash[:notice] %>
      <%= yield %>
    </body> 
  <% else %>
    <%= yield %>
  <% end %>
   
 
    
  
</html>
